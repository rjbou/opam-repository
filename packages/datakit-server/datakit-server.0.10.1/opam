  "fmt"
  "jbuilder" {build & < "1.0+beta12"}
  "logs"
  "mirage-flow-lwt"
  "ocaml"
  "prometheus"
  "protocol-9p-unix" {>= "0.11.0"}
  "rresult"
  "sexplib" {< "v0.13"}
  "uri"
]
build: ["jbuilder" "build" "-p" name "-j" jobs]
