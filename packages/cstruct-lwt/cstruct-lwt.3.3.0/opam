bug-reports: "https://github.com/mirage/ocaml-cstruct/issues"
depends: [
  "base-unix"
  "cstruct" {>= "3.2.0" & < "3.4.0"}
  "dune" {build & >= "1.0"}
  "lwt"
  "ocaml" {>= "4.03.0"}
]
build: [
  ["dune" "subst"] {pinned}
