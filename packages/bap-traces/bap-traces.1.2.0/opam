depends: [
  "bap-std" {= "1.2.0"}
  "oasis" {build & >= "0.4.7"}
  "ocaml"
  "uri" {>= "1.9.0"}
  "uuidm"
]
flags: light-uninstall
build: [
  ["./configure" "--prefix=%{prefix}%" "--mandir=%{man}%" "--enable-traces"]
  [make]
]
install: [make "install"]
remove: ["ocamlfind" "remove" "bap-traces"]
