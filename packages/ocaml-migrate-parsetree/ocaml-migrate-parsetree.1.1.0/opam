doc: "https://ocaml-ppx.github.io/ocaml-migrate-parsetree/"
bug-reports: "https://github.com/ocaml-ppx/ocaml-migrate-parsetree/issues"
depends: [
  "dune" {build}
  "ocaml" {>= "4.02.0" & < "4.08.0"}
  "result"
]
build: ["jbuilder" "build" "-p" name "-j" jobs]
