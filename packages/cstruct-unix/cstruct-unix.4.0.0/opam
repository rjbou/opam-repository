  "Richard Mortier"
  "Thomas Gazagnaire"
  "Thomas Leonard"
]
license: "ISC"
tags: ["org:mirage" "org:ocamllabs"]
homepage: "https://github.com/mirage/ocaml-cstruct"
doc: "https://mirage.github.io/ocaml-cstruct/"
bug-reports: "https://github.com/mirage/ocaml-cstruct/issues"
depends: [
  "base-unix"
  "cstruct" {= version}
  "dune" {build & >= "1.0"}
  "ocaml" {>= "4.06.0"}
]
build: [
  ["dune" "subst"] {pinned}
  ["dune" "build" "-p" name "-j" jobs]
]
