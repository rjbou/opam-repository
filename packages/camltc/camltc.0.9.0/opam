depends: [
  "lwt" {>= "2.4.3" & < "4.0.0"}
  "ocaml" {> "4.00.0" & < "4.06.0"}
  "ocamlbuild" {build}
  "ocamlfind"
  "ounit"
]
build: [make "-C" "src"]
install: [make "-C" "src" "install" "DESTDIR=%{prefix}%"]
