  "camlp4"
  "ocaml" {< "4.02.0"}
  "ocamlbuild" {build}
  "ocamlfind"
  "pa_ounit" {= "109.14.00"}
  "sexplib" {= "109.14.00"}
  "type_conv" {= "109.14.00"}
]
flags: light-uninstall
build: make
install: [make "install"]
remove: ["ocamlfind" "remove" "custom_printf"]
