depends: [
  "base-bytes" {with-test}
  "dune" {build}
  "fmt" {with-test}
  "ocaml" {>= "4.03.0"}
]
build: [
  ["dune" "subst"]
  ["dune" "build" "-p" name "-j" jobs]
  ["dune" "runtest" "-p" name "-j" jobs] {with-test}
]
