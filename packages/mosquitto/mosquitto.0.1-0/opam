  "base-bytes"
  "base-unix"
  "conf-libmosquitto"
  "ocaml"
  "ocamlbuild" {build}
  "ocamlfind" {>= "1.5"}
  "result"
]
flags: light-uninstall
build: [
  ["ocaml" "setup.ml" "-configure"]
  ["ocaml" "setup.ml" "-build"]
  ["ocaml" "setup.ml" "-doc"] {with-doc}
]
